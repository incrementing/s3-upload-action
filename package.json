{
  "name": "s3-upload-action",
  "version": "2.1.0",
  "description": "This is a GitHub Action that uploads a file to Amazon S3",
  "main": "src/main.js",
  "author": "Hiroyuki Kusu <kusu0806@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hkusu/s3-upload-action.git"
  },
  "bugs": {
    "url": "https://github.com/hkusu/s3-upload-action/issues"
  },
  "homepage": "https://github.com/hkusu/s3-upload-action#readme",
  "scripts": {
    "test": "rm -rf dist/ && npx esbuild src/main.js --bundle --platform=node --outfile=dist/index.js && act --job=upload --use-gitignore=false",
    "package": "rm -rf dist/ && npx esbuild src/main.js --bundle --platform=node --outfile=dist/index.js --minify"
  },
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@aws-sdk/client-s3": "^3.940.0",
    "@aws-sdk/s3-request-presigner": "^3.940.0",
    "qrcode": "^1.5.4"
  },
  "devDependencies": {
    "esbuild": "^0.27.0"
  }
}
